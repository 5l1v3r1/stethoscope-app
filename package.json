{
  "name": "Stethoscope",
  "version": "0.3.2",
  "private": true,
  "homepage": "./",
  "author": "Netflix",
  "description": "Local Stethoscope app",
  "main": "src/start.js",
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://s3-us-west-2.amazonaws.com/nflx-stethoscope-public-securityprod-us-west-2/"
      }
    ],
    "appId": "com.netflix.stethoscope-local",
    "extends": null,
    "productName": "Stethoscope",
    "files": [
      "bin/*",
      "practices/*",
      "build/**",
      "public/**",
      "resolvers/*",
      "sources/*",
      "src/lib/*.js",
      "src/*.json",
      "src/*.js",
      "server.js",
      "schema.graphql"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis",
      "certificateSubjectName": "Netflix, Inc."
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": "AppImage",
      "category": "System"
    },
    "protocols": [
      {
        "name": "stethoscope",
        "role": "Viewer",
        "schemes": [
          "stethoscope"
        ]
      }
    ]
  },
  "scripts": {
    "start": "nf start -p 12000",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "react-scripts start",
    "build": "rm -r dist/ ; react-scripts build && electron-builder -mw",
    "postinstall": "electron-builder install-app-deps",
    "build:mac": "rm -r dist/ ; react-scripts build && electron-builder -m",
    "build:windows": "react-scripts build && electron-builder -w",
    "codesign:mac": "echo TODO",
    "codesign:windows": "/usr/local/bin/signcode -$ commercial -a sha1 -t http://timestamp.digicert.com -i https://stethoscope.prod.netflix.net -spc code-cert.spc -v code-cert.pvk -n \"Stethoscope Installer\" \"dist/Stethoscope Setup $npm_package_version.exe\"",
    "build:linux": "rm -r dist/ ; react-scripts build && electron-builder -l",
    "nsp": "nsp check"
  },
  "dependencies": {
    "applescript": "^1.0.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "electron-log": "^2.2.11",
    "electron-updater": "^2.20.1",
    "es6-promisify": "^6.0.0",
    "express": "^4.16.2",
    "foreman": "^2.0.0",
    "graphql": "^0.13.2",
    "graphql-server-express": "^1.2.0",
    "graphql-tools": "^2.12.0",
    "helmet": "^3.9.0",
    "js-yaml": "^3.10.0",
    "marked": "^0.3.7",
    "moment": "^2.19.3",
    "node-powershell": "^3.3.1",
    "nsp": "^3.2.1",
    "qr-image": "^3.2.0",
    "qrcode": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.4",
    "semver": "^5.4.1",
    "shell-escape": "^0.2.0",
    "showdown": "^1.8.6",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "electron": "^1.7.11",
    "electron-builder": "^20.8.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1"
  }
}
